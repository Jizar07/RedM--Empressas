{
  "name": "redm-discord-bot",
  "version": "0.001",
  "description": "Discord bot for RedM server management with web dashboard",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx nodemon",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "typecheck": "npx tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:connection": "npx tsc src/test-connection.ts --outDir dist --esModuleInterop --resolveJsonModule --skipLibCheck && node dist/test-connection.js"
  },
  "keywords": [
    "discord",
    "bot",
    "redm",
    "fivem",
    "dashboard"
  ],
  "author": "Jizar07",
  "license": "MIT",
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pm2": "^6.0.8",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
